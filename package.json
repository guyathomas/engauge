{
  "name": "engauge",
  "version": "1.0.0",
  "description": "Track where users are looking on your designs",
  "main": "index.js",
  "engines": {
    "node": "6.1.0"
  },
  "scripts": {
    "start": "node ./src/server/index.js",
    "relay": "relay-compiler --src ./src/client/dev --schema schema.graphql",
    "get-schema": "node ./scripts/getSchema.js",
    "build": "webpack -d;",
    "dev": "concurrently \"webpack -d --watch\" \"npm start\"",
    "test:unit": "mocha test './test/unit/**/*.spec.js'",
    "test": "npm run test:unit"
  },
  "author": "Guy Thomas",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "concurrently": "^3.1.0",
    "express": "^4.14.0",
    "express-graphql": "^0.6.5",
    "graphiql": "^0.10.2",
    "graphql": "^0.9.6",
    "graphql-relay": "^0.5.1",
    "graphql-sequelize": "^5.3.2",
    "graphql-type-json": "^0.1.4",
    "lodash": "^4.17.4",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "relay-compiler": "^1.0.0",
    "sequelize": "^3.27.0",
    "socket.io": "^1.7.3",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-stage-2": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0"
  }
}
