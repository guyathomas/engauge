{
  "name": "engauge",
  "version": "1.0.0",
  "description": "Track where users are looking on your designs",
  "main": "index.js",
  "engines": {
    "node": "6.1.0"
  },
  "scripts": {
    "start": "node ./src/server/index.js",
    "relay": "relay-compiler --src ./src/client/dev --schema schema.graphql",
    "get-schema": "node ./scripts/getSchema.js",
    "build": "webpack -d;",
    "dev": "concurrently \"webpack -d --watch\" \"npm start\"",
